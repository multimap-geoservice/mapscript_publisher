        "layer_dicts": [
            {
                "layer_temp": "landuse",
                "name": "landuse",
                "data": {
                    "RUN":{
                        "interface.lst2str":[
                            "geometry from",
                            "(",
                            "select way as geometry,",
                            "osm_id,",
                            "concat(",
                            "    landuse,",
                            "    \"natural\",",
                            "    leisure,",
                            "    highway,",
                            "    amenity",
                            ") as type,",
                            "name as name",
                            "from planet_osm_polygon",
                            "where landuse in",
                            "(",
                            "    'forest',",
                            "    'cemetery',",
                            "    'industrial',",
                            "    'commercial',",
                            "    'brownfield',",
                            "    'residential',",
                            "    'military'",
                            ")",
                            "or leisure in",
                            "(",
                            "    'park',",
                            "    'golf_course',",
                            "    'stadium',",
                            "    'sports_center',",
                            "    'pitch'",
                            ")",
                            "or amenity in",
                            "(",
                            "    'hospital',",
                            "    'school',",
                            "    'college',",
                            "    'university',",
                            "    'parking'",
                            ")",
                            "or \"natural\" = 'wood'",
                            "or highway = 'pedestrian'",
                            "order by area desc",
                            ")",
                            {
                                "VAR": "end_qury_str",
                                "{}": {
                                    "VAR":"epsg_id"
                                }
                            }
                        ]
                    }
                },
                "wms_abstract": "landuse"
            }, 
            {
                "layer_temp": "buildings",
                "name": "buildings",
                "data": {
                    "RUN":{
                        "interface.lst2str":[
                            "geometry from",
                            "(",
                            "select way as geometry,",
                            "osm_id,",
                            "name as name",
                            "from planet_osm_polygon",
                            "where building != ''",
                            ")",
                            "as foo using unique osm_id using srid=3857"
                        ]
                    }
                },
                "wms_abstract": "buildings"
            } 
        ]
    },
